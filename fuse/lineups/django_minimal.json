{
    "home": {
        "project_home": "~/test"
    },
    "virtualenv": {
        "label": "(env) ",
        "python_distribution": "3",
        "directory": "env/",
	"retrigger": false
    },
    "django": {
        "project_name": "Sample Project",
        "project_identifier": "sample_project",
        "version": "1.10.5",
        "settings_file": "{project_home}/{project_identifier}/settings.py",
        "init_file": "{project_home}/{project_identifier}/__init__.py",
        "urls_file": "{project_home}/{project_identifier}/urls.py",
        "wsgi_file": "{project_home}/{project_identifier}/wsgi.py",
        "manage_file": "{project_home}/manage.py"
    }
}
