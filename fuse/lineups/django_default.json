{
    "home": {
        "project_home": "~/test"
    },
    "django": {
        "project_name": "Sample Project",
        "project_identifier": "sample_project",
        "project_environments": "development,production",
        "version": "1.10.5",
        "settings_file": "{project_home}/{project_identifier}/settings.py",
        "init_file": "{project_home}/{project_identifier}/__init__.py",
        "urls_file": "{project_home}/{project_identifier}/urls.py",
        "wsgi_file": "{project_home}/{project_identifier}/wsgi.py",
        "manage_file": "{project_home}/manage.py"
    },
    "virtualenv": {
        "prompt": "(env) ",
        "python_distribution": "3",
        "directory": "env/",
        "retrigger": false
    },
    "pip": {
        "requirements_target": "requirements.txt"
    },
    "git": {
    },
    "celery": {
	"version": "4.0.2",
	"setup_file": "{project_home}/{project_identifier}/taskrunner/celery.py",
	"environment:0": "development",
	"broker:0": "amqp://guest:**@localhost:5672",
	"environment:1": "production",
	"broker:1": "amqp://guest:**@localhost:5672"
    }
}
