{
    "home": {
        "project_home": "~/test"
    },
    "django": {
        "project_name": "Sample Project",
        "project_slug": "sample_project",
        "version": "1.10.5",
        "project_template_root": "https://raw.githubusercontent.com/django/django/{version}/django/conf/project_template/",
        "settings_file": {
            "source": "project_name/settings.py-tpl",
            "target": "{project_slug}/settings.py"
        },
        "init_file": {
            "source": "project_name/__init__.py-tpl",
            "target": "{project_slug}/__init__.py"
        },
        "urls_file": {
            "source": "project_name/urls.py-tpl",
            "target": "{project_slug}/urls.py"
        },
        "wsgi_file": {
            "source": "project_name/wsgi.py-tpl",
            "target": "{project_slug}/wsgi.py"
        },
        "manage_file": {
            "source": "manage.py-tpl",
            "target": "manage.py"
        }
    },
    "virtualenv": {
        "prompt": "(env) ",
        "python_distribution": "3",
        "directory": "env/"
    },
    "pip": {
        "requirements_target": "requirements.txt"
    },
    "git": {
    }
}
